<MudCard>
    <MudCardContent>
        <MudText>@Title</MudText>
        <MudText Typo="Typo.body2">
        </MudText>
    </MudCardContent>
    <MudCardActions>
        <MudButton Variant="Variant.Text" Color="Color.Primary">Learn More</MudButton>
    </MudCardActions>
</MudCard>

@code {
    [Parameter]
    public string Title { get; set; } = default!;
}
